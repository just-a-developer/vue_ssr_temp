{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 5 versions",
                        "not ie <= 8"
                    ]
                },
                "useBuiltIns": "usage",
                "corejs": 2
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 2
            }
        ],
        "@babel/plugin-syntax-dynamic-import"
    ]
}